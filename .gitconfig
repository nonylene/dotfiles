[include]
    path = ~/.gituser
[filter "media"]
    clean = git-media-clean %f
    smudge = git-media-smudge %f
[color]
    ui = auto
[alias]
    co = checkout
    cob = checkout -b
    br = branch
    cm = commit -m
    ca = commit --amend
    com = checkout master
    cod = checkout dev
    cp = cherry-pick
    sts = status
    df = diff
    dfc = diff --cached
    pp = pull --prune
[core]
    pager = less
    autocrlf = input
    excludesfile = ~/.gitignore
